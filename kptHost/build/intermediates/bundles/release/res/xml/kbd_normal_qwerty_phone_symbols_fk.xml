<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:horizontalGap="5%p"
    
    atx:fontSizeApplicable="true"
    atx:fontStyleApplicable="true"
    atx:keyType="@integer/kpt_key_type_normal"
    
    atx:isRTL="false"
    
    atx:keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize_phone_sym"
    atx:keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:keyLabelTextSize="@dimen/kpt_keyLabelTextSize_phone_sym"
    atx:keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:keyLabelYPos="@dimen/kpt_keyLabelYPos"
    
    atx:isGlideSupported = "false"
    
    atx:keyWidth="21%p"
    atx:keyHeight="@dimen/kpt_key_height_dk_fk"
    atx:verticalGap="0dp" >

    <Row>
        <Key
            atx:horizontalGap="3.5%p"
            atx:keyEdgeFlags="left"
            atx:keyLabel="(" />
        <Key atx:keyLabel="/" />
        <Key atx:keyLabel=")" />
        <Key
            atx:keyEdgeFlags="right"
            atx:keyLabel="-" />
    </Row>
    <Row>
        <Key
            atx:horizontalGap="3.5%p"
            atx:keyEdgeFlags="left"
            atx:keyLabel="N" />
        <!--
             Pause is a comma. 
                Check PhoneNumberUtils.java to see if this has changed.
        -->
        <Key
            atx:codes="44"
            atx:keyLabel="Pause" />
        <Key atx:keyLabel="," />
        <Key
            atx:codes="46"
            atx:keyLabel="."
            atx:keyEdgeFlags="right" />
    </Row>
    <Row>
        <Key
            atx:codes="42"
            atx:horizontalGap="3.5%p"
            atx:keyEdgeFlags="left"
            atx:keyLabel="*" />
        <!-- Wait is a semicolon. -->
        <Key
            atx:codes="59"
            atx:keyLabel="Wait" />
        <Key
            atx:codes="35"
            atx:keyLabel="\#" />
        <Key
            atx:codes="-5"
            atx:keyType="@integer/kpt_functional_key_dont_send_to_core"
            atx:iconText="@string/kpt_icontext_delete"
            atx:isRepeatable="true"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>
    <Row atx:rowEdgeFlags="bottom" >
        <Key
            atx:codes="-2"
            atx:horizontalGap="3.5%p"
            atx:keyEdgeFlags="left"
            atx:keyLabel="@string/kpt_label_phone_key" />
        <Key
            atx:codes="48,43"
            atx:keyLabel="0"
            atx:keyLabel2="+" />
        <Key
            atx:codes="32"
            atx:iconText="@string/kpt_icontext_space"
            atx:useExternalFont="true" />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyEdgeFlags="right"
            atx:useExternalFont="true" />
    </Row>

</Keyboard><!-- From: file:/Users/gauraw/hike/Fork-Hike/android-client/kptHost/res/xml/kbd_normal_qwerty_phone_symbols_fk.xml -->