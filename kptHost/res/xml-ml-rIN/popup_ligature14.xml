<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:kpt_keyWidth="10%p"
    atx:kpt_horizontalGap="0dp"
    atx:kpt_verticalGap="0dp"
    atx:kpt_keyHeight="@dimen/kpt_key_height"
    >

    <Row atx:kpt_rowEdgeFlags="top">
        <Key atx:kpt_keyLabel="ഠ" atx:kpt_keyOutputText="ഠ" atx:kpt_keyEdgeFlags="left" />
        <Key atx:kpt_codes="-1073" atx:kpt_keyLabel="ച്ച" atx:kpt_keyOutputText="ച്ച" atx:kpt_keyEdgeFlags="right" />
    </Row>
</Keyboard>
