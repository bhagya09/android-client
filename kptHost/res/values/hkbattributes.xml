<!-- This xml Specifies the android format styleables for the HKB keymap resource file. -->

<resources>

	<declare-styleable name="Kpt_HardKeyboardMap">
	<!-- The script of this HKB keymap. -->
        <attr name="kpt_version" format="float" />
	</declare-styleable>

	<declare-styleable name="Kpt_HardKeyboardMap_Keymap">
        <!-- The script of this HKB keymap. -->
        <attr name="kpt_locale" format="string" />
    </declare-styleable>

    <declare-styleable name="Kpt_HardKeyboardMap_Key">
        <!-- The Key code. -->
        <attr name="kpt_code" format="integer" />
        
        <!-- The Key label -->
        <attr name="kpt_label" format="string" />
        
        <!-- The accented character values. -->
        <attr name="kpt_values" format="string" />
    </declare-styleable>

</resources>
