        <Key
            atx:codes="-17"
            atx:keyLabel="@string/kpt_label_alpha_key"
              atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p" />
        
        <Key
            atx:codes="-23"
            atx:keyLabel="@string/kpt_english_locale"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p"
            atx:useExternalFont="true" />
       <!--  <Key
            atx:codes="-103"
            atx:iconText="@string/kpt_icontext_mic"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p"
            atx:useExternalFont="true" /> -->
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p"
            atx:useExternalFont="true" />
    </Row>
    
    <Row
        atx:keyboardMode="@+id/phone_last_row_no_voice_hide_xi"
        atx:rowEdgeFlags="bottom" >
        <Key
            atx:codes="-1"
            atx:iconText="@string/kpt_icontext_shift_off"
            atx:isModifier="true"
            atx:isSticky="true"
            atx:keyEdgeFlags="left"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32,48"
            atx:iconText="@string/kpt_icontext_space_zero"
            atx:keyIcon="@drawable/kpt_space_icon"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="50%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="-17"
            atx:keyLabel="@string/kpt_label_alpha_key"
            atx:keyWidth="12.5%p" />

        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>
    <Row
        atx:keyboardMode="@+id/phone_last_row_with_voice_hide_xi"
        atx:rowEdgeFlags="bottom" >
        <Key
            atx:codes="-1"
            atx:iconText="@string/kpt_icontext_shift_off"
            atx:isModifier="true"
            atx:isSticky="true"
            atx:keyEdgeFlags="left"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32,48"
            atx:iconText="@string/kpt_icontext_space_zero"
            atx:keyIcon="@drawable/kpt_space_icon"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="50%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="-17"
            atx:keyLabel="@string/kpt_label_alpha_key"
            atx:keyWidth="12.5%p" />

       <!--  <Key
            atx:codes="-103"
            atx:iconText="@string/kpt_icontext_mic"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p"
            atx:useExternalFont="true" /> -->
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="12.5%p"
            atx:useExternalFont="true" />
