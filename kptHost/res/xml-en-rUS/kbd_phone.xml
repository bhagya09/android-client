<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:horizontalGap="0dp"
    atx:keyHeight="@dimen/kpt_key_height_dk_fk"
    
    atx:keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize"
    atx:keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:keyLabelTextSize="@dimen/kpt_keyLabelTextSize"
    atx:keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:keyLabelYPos="@dimen/kpt_keyLabelYPos"
    
    atx:isGlideSupported = "false"
    atx:isRTL = "false"
    atx:fontStyleApplicable = "true"
    atx:fontSizeApplicable = "true"
    
    atx:keyType="@integer/kpt_key_type_normal"
    
    atx:keyWidth="25%p"
    atx:verticalGap="0dp" >
	<Row atx:isNumberRow = "true"
	    atx:keyLabelYPos="@dimen/kpt_keyLabelNumYPos"
       atx:keyHeight="@dimen/kpt_number_row_key_height_dk_fk"
       atx:keyWidth="10%p"
        >
        <Key
            atx:codes="49"
            atx:keyEdgeFlags="left"
           
            atx:keyLabel="1"
             />
        <Key
            atx:codes="50"
            atx:keyLabel="2"
             />
        <Key
            atx:codes="51"
            atx:keyLabel="3"
           />
        <Key
            atx:codes="52"
            atx:keyLabel="4"
            />
        <Key
            atx:codes="53"
            atx:keyLabel="5"
          />
        <Key
            atx:codes="54"
            atx:keyLabel="6"       
            />
        <Key
            atx:codes="55"
            atx:keyLabel="7"
          
            />
        <Key
            atx:codes="56"
            atx:keyLabel="8"
         />
        <Key
            atx:codes="57"
            atx:keyLabel="9"
         
         />
        <Key
            atx:codes="48"
            atx:keyEdgeFlags="right"
            atx:keyLabel="0"
          />
    </Row>
    <!-- First row -->
    <Row
        atx:keyboardMode="@+id/phone_first_row_normal"
        atx:rowEdgeFlags="top" >
        <Key
            atx:codes="46,44,63,33,39,34,49,45,40,41,64,47,58,95,35,43"
            atx:keyEdgeFlags="left"
            atx:keyLabel=".,?!"
            atx:keyLabel2="1"
            atx:popupCharacters="@string/kpt_alternates_for_12key_normal"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="97,98,99,50,225,224,226,230,228,227,231"
            atx:keyLabel="abc"
            atx:keyLabel2="2"
            atx:popupCharacters="@string/kpt_alternates_for_abc"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="100,101,102,51,233,232,234,235"
            atx:keyLabel="def"
            atx:keyLabel2="3"
            atx:popupCharacters="@string/kpt_alternates_for_def"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="-5"
            atx:iconText="@string/kpt_icontext_delete"
            atx:isRepeatable="true"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>
    <Row
        atx:keyboardMode="@+id/phone_first_row_email"
        atx:rowEdgeFlags="top" >
        <Key
            atx:codes="64,46,45,95,44,63,33,39,34,49,40,41,47,58,35,43"
            atx:keyEdgeFlags="left"
            atx:keyLabel="\@.-"
            atx:keyLabel2="1"
            atx:popupCharacters="@string/kpt_alternates_for_12key_email"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="97,98,99,50,225,224,226,230,228,227,231"
            atx:keyLabel="abc"
            atx:keyLabel2="2"
            atx:popupCharacters="@string/kpt_alternates_for_abc"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="100,101,102,51,233,232,234,235"
            atx:keyLabel="def"
            atx:keyLabel2="3"
            atx:popupCharacters="@string/kpt_alternates_for_def"
