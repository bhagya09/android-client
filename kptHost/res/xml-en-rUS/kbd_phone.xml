        <Key
            atx:codes="-15"
            atx:keyEdgeFlags="right"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk" />
    </Row>

    <!-- third row -->
    <Row>
        <Key
            atx:codes="112,113,114,115,55,223"
            atx:keyEdgeFlags="left"
            atx:keyLabel="pqrs"
            atx:keyLabel2="7"
            atx:popupCharacters="@string/kpt_alternates_for_pqrs"
