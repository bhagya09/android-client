        <Key
            atx:codes="-5"
            atx:iconText="@string/kpt_icontext_delete"
            atx:isRepeatable="true"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>
    <Row
        atx:keyboardMode="@+id/phone_first_row_url"
        atx:rowEdgeFlags="top" >
        <Key
            atx:codes="46,47,45,95,44,63,33,39,34,49,40,41,64,58,35,43"
            atx:keyEdgeFlags="left"
            atx:keyLabel="./-"
            atx:keyLabel2="1"
            atx:popupCharacters="@string/kpt_alternates_for_12key_url"
