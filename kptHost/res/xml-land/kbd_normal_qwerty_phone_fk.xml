<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:kpt_horizontalGap="0%p"
    atx:kpt_keyHeight="@dimen/kpt_key_height_dk_fk"
    atx:kpt_keyWidth="32%p"
   
    atx:kpt_keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelYPos"
    atx:kpt_keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:kpt_keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:kpt_keyLabelTextSize="@dimen/kpt_keyLabelTextSize"
    atx:kpt_keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize"
    
    atx:kpt_isGlideSupported = "true"
    
    atx:kpt_keyType="@integer/kpt_key_type_normal"
    atx:kpt_isRTL = "false"
    atx:kpt_fontStyleApplicable = "true"
    atx:kpt_fontSizeApplicable = "true"
    atx:kpt_verticalGap="0%p" >

    <Row>
        <Key
            atx:kpt_codes="49"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="1" />
        <Key
            atx:kpt_codes="50"
            atx:kpt_keyLabel="2" />
        <Key
            atx:kpt_codes="51"
            atx:kpt_keyLabel="3" 
            atx:kpt_keyEdgeFlags="right"/>
       
    </Row>
    <Row>
        <Key
            atx:kpt_codes="52"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="4" />
        <Key
            atx:kpt_codes="53"
            atx:kpt_keyLabel="5" />
        <Key
            atx:kpt_codes="54"
            atx:kpt_keyLabel="6" 
            atx:kpt_keyEdgeFlags="right"/>
        
    </Row>
    <Row>
        <Key
            atx:kpt_codes="55"
            atx:kpt_horizontalGap="3.5%p"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="7" />
        <Key
            atx:kpt_codes="56"
            atx:kpt_keyLabel="8" />
        <Key
            atx:kpt_codes="57"
            atx:kpt_keyLabel="9" 
            atx:kpt_keyEdgeFlags="right"/>
        
    </Row>
    <Row atx:kpt_rowEdgeFlags="bottom"
        atx:kpt_keyType="@integer/kpt_functional_key_dont_send_to_core" >
        <Key
            atx:kpt_codes="-5"
            atx:kpt_keyType="@integer/kpt_functional_key_dont_send_to_core"
            atx:kpt_iconText="@string/kpt_icontext_delete"
            atx:kpt_isRepeatable="true"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_useExternalFont="true" />    
       
        <Key
            atx:kpt_codes="48"
            atx:kpt_keyLabel="0"
             />
        
        
        <Key
            atx:kpt_codes="10"
            atx:kpt_iconText="@string/kpt_icontext_enter_done"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_useExternalFont="true" />
    </Row>

</Keyboard>