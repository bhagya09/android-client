<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:kpt_horizontalGap="0dp"
    atx:kpt_keyHeight="@dimen/kpt_key_height_dk_fk"
    
    atx:kpt_keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize"
    atx:kpt_keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:kpt_keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:kpt_keyLabelTextSize="@dimen/kpt_keyLabelTextSize"
    atx:kpt_keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelYPos"
    
    atx:kpt_isGlideSupported = "false"
    atx:kpt_keyType="@integer/kpt_key_type_normal"
    atx:kpt_isRTL = "false"
    atx:kpt_fontStyleApplicable = "true"
    atx:kpt_fontSizeApplicable = "true"
    
    atx:kpt_keyWidth="25%p"
    atx:kpt_verticalGap="0dp" >
    <Row atx:kpt_isNumberRow = "true"
        atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelNumYPos"
       atx:kpt_keyHeight="@dimen/kpt_number_row_key_height_dk_fk"
       atx:kpt_keyWidth="10%p"
        >
        <Key
            atx:kpt_codes="49"
            atx:kpt_keyEdgeFlags="left"
           
            atx:kpt_keyLabel="1"
             />
        <Key
            atx:kpt_codes="50"
            atx:kpt_keyLabel="2"
             />
        <Key
            atx:kpt_codes="51"
            atx:kpt_keyLabel="3"
           />
        <Key
            atx:kpt_codes="52"
            atx:kpt_keyLabel="4"
            />
        <Key
            atx:kpt_codes="53"
            atx:kpt_keyLabel="5"
          />
        <Key
            atx:kpt_codes="54"
            atx:kpt_keyLabel="6"       
            />
        <Key
            atx:kpt_codes="55"
            atx:kpt_keyLabel="7"
          
            />
        <Key
            atx:kpt_codes="56"
            atx:kpt_keyLabel="8"
         />
        <Key
            atx:kpt_codes="57"
            atx:kpt_keyLabel="9"
         
         />
        <Key
            atx:kpt_codes="48"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabel="0"
          />
    </Row>
    <!-- First row -->
    <Row
        atx:kpt_keyboardMode="@+id/phone_first_row_normal"
        atx:kpt_rowEdgeFlags="top" >
        <Key
            atx:kpt_codes="46,124,44,63,33,39,34,49,45,40,41,64,47,58,95,35,43,8360 "
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel=".,?!"
            atx:kpt_keyLabel2="1"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_12key_normal"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="97,98,99,50,225,224,226,230,228,227,231"
            atx:kpt_keyLabel="abc"
            atx:kpt_keyLabel2="2"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_abc"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="100,101,102,51,233,232,234,235"
            atx:kpt_keyLabel="def"
            atx:kpt_keyLabel2="3"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_def"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="-5"
            atx:kpt_iconText="@string/kpt_icontext_delete"
            atx:kpt_isRepeatable="true"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>
    <Row
        atx:kpt_keyboardMode="@+id/phone_first_row_email"
        atx:kpt_rowEdgeFlags="top" >
        <Key
            atx:kpt_codes="64,46,124,45,95,44,63,33,39,34,49,40,41,47,58,35,43,8360 "
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="\@.-"
            atx:kpt_keyLabel2="1"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_12key_email"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="97,98,99,50,225,224,226,230,228,227,231"
            atx:kpt_keyLabel="abc"
            atx:kpt_keyLabel2="2"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_abc"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="100,101,102,51,233,232,234,235"
            atx:kpt_keyLabel="def"
            atx:kpt_keyLabel2="3"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_def"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="-5"
            atx:kpt_iconText="@string/kpt_icontext_delete"
            atx:kpt_isRepeatable="true"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>
    <Row
        atx:kpt_keyboardMode="@+id/phone_first_row_url"
        atx:kpt_rowEdgeFlags="top" >
        <Key
            atx:kpt_codes="46,124,47,45,95,44,63,33,39,34,49,40,41,64,58,35,43,8360 "
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="./-"
            atx:kpt_keyLabel2="1"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_12key_url"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="97,98,99,50,225,224,226,230,228,227,231"
            atx:kpt_keyLabel="abc"
            atx:kpt_keyLabel2="2"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_abc"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="100,101,102,51,233,232,234,235"
            atx:kpt_keyLabel="def"
            atx:kpt_keyLabel2="3"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_def"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="-5"
            atx:kpt_iconText="@string/kpt_icontext_delete"
            atx:kpt_isRepeatable="true"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>

    <!-- Second row -->
    <Row>
        <Key
            atx:kpt_codes="103,104,105,52,236,237,238,239"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="ghi"
            atx:kpt_keyLabel2="4"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_ghi"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="106,107,108,53"
            atx:kpt_keyLabel="jkl"
            atx:kpt_keyLabel2="5"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_jkl"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="109,110,111,54,241,243,242,244,246,245,339"
            atx:kpt_keyLabel="mno"
            atx:kpt_keyLabel2="6"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_mno"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="-15"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyIcon="@drawable/kpt_adaptxt_settings_fk" />
    </Row>

    <!-- third row -->
    <Row>
        <Key
            atx:kpt_codes="112,113,114,115,55,223"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="pqrs"
            atx:kpt_keyLabel2="7"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_pqrs"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="116,117,118,56,250,252,249,251"
            atx:kpt_keyLabel="tuv"
            atx:kpt_keyLabel2="8"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_tuv"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="119,120,121,122,57,255"
            atx:kpt_keyLabel="wxyz"
            atx:kpt_keyLabel2="9"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_wxyz"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="-13"
            atx:kpt_iconText="@string/kpt_icontext_globe"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>

    <!-- Fourth row. check with or without voice -->

   
    

     <Row
        atx:kpt_keyboardMode="@+id/phone_last_row_no_voice_hide_xi"
        atx:kpt_rowEdgeFlags="bottom" >
        <Key
            atx:kpt_codes="-1"
            atx:kpt_iconText="@string/kpt_icontext_shift_off"
            atx:kpt_isModifier="true"
            atx:kpt_isSticky="true"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="32,48"
            atx:kpt_iconText="@string/kpt_icontext_space_zero"
            atx:kpt_keyIcon="@drawable/kpt_space_icon"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="37.5%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="-17"
            atx:kpt_keyLabel="@string/kpt_label_alpha_key"
            atx:kpt_keyWidth="12.5%p" />

        <Key
            atx:kpt_codes="10"
            atx:kpt_iconText="@string/kpt_icontext_enter_kbhide"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>
    <Row
        atx:kpt_keyboardMode="@+id/phone_last_row_with_voice_hide_xi"
        atx:kpt_rowEdgeFlags="bottom" >
        <Key
            atx:kpt_codes="-1"
            atx:kpt_iconText="@string/kpt_icontext_shift_off"
            atx:kpt_isModifier="true"
            atx:kpt_isSticky="true"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="32,48"
            atx:kpt_iconText="@string/kpt_icontext_space_zero"
            atx:kpt_keyIcon="@drawable/kpt_space_icon"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="50%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="-17"
            atx:kpt_keyLabel="@string/kpt_label_alpha_key"
            atx:kpt_keyWidth="12.5%p" />

        <!-- <Key
            atx:kpt_codes="-103"
            atx:kpt_iconText="@string/kpt_icontext_mic"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_keyWidth="12.5%p"
            atx:kpt_useExternalFont="true" /> -->
        <Key
            atx:kpt_codes="10"
            atx:kpt_iconText="@string/kpt_icontext_enter_kbhide"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_keyWidth="12.5%p"
            atx:kpt_useExternalFont="true" />
    </Row>
</Keyboard>