<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:horizontalGap="0dp"
    atx:keyHeight="@dimen/kpt_key_height"
    
    atx:keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize_phone_sym"
    atx:keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:keyLabelTextSize="@dimen/kpt_keyLabelTextSize_phone_sym"
    atx:keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:keyLabelYPos="@dimen/kpt_keyLabelYPos"
    
    atx:keyType="@integer/kpt_key_type_normal"
    atx:isGlideSupported = "false"
    
    atx:keyWidth="25%p"
    atx:verticalGap="0dp" >

    <Row>
        <Key
            atx:codes="49"
            atx:keyEdgeFlags="left"
            atx:keyLabel="1" />
        <Key
            atx:codes="50"
            atx:keyLabel="2" />
        <Key
            atx:codes="51"
            atx:keyLabel="3" />
        <Key
            atx:codes="-5"
            atx:iconText="@string/kpt_icontext_delete"
            atx:isRepeatable="true"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>
    <Row>
        <Key
            atx:codes="52"
            atx:keyEdgeFlags="left"
            atx:keyLabel="4" />
        <Key
            atx:codes="53"
            atx:keyLabel="5" />
        <Key
            atx:codes="54"
            atx:keyLabel="6" />
        <Key
            atx:codes="-15"
            atx:keyEdgeFlags="right"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk" />
    </Row>
    <Row>
        <Key
            atx:codes="55"
            atx:keyEdgeFlags="left"
            atx:keyLabel="7" />
        <Key
            atx:codes="56"
            atx:keyLabel="8" />
        <Key
            atx:codes="57"
            atx:keyLabel="9" />
        <Key
            atx:codes="-13"
            atx:iconText="@string/kpt_icontext_globe"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:keyLabelTextSize="@dimen/kpt_keyLabel2GlobeShareTextSize"
            atx:useExternalFont="true" />
    </Row>
    <Row atx:rowEdgeFlags="bottom" >
        <Key
            atx:codes="-16"
            atx:keyLabel="@string/kpt_phone_mode_change_to_sym_shift" />
        <Key
            atx:codes="48,43"
            atx:keyLabel="0 +" />
        <Key
            atx:codes="46,44"
            atx:keyLabel=".," />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyEdgeFlags="right"
            atx:keyLabelYPos="0.50"
            atx:useExternalFont="true" />
    </Row>

</Keyboard>