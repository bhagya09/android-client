<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:horizontalGap="0dp"
    atx:keyHeight="@dimen/kpt_key_height_dk_fk"
    
    atx:keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:keyLabelYPos="@dimen/kpt_keyLabelYPos"
    atx:keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:keyLabelTextSize="@dimen/kpt_keyLabelTextSize"
    atx:keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize"
    
    atx:isGlideSupported = "false"
    atx:isRTL = "false"
    atx:fontStyleApplicable = "true"
    atx:fontSizeApplicable = "true"
    
    atx:keyType="@integer/kpt_key_type_normal"
    
    atx:keyWidth="10%p"
    atx:verticalGap="@dimen/kpt_vertical_gap" >

    <Row>
        <Key
            atx:codes="123,113"
            atx:keyEdgeFlags="left"
            atx:keyLabel="{"
            atx:keyLabel2="q" />
        <Key
            atx:codes="125,119"
            atx:keyLabel="}"
            atx:keyLabel2="w" />
        <Key
            atx:codes="8734,101"
            atx:keyLabel="∞"
            atx:keyLabel2="e" />
        <Key
            atx:codes="174,114"
            atx:keyLabel="®"
            atx:keyLabel2="r" />
        <Key
            atx:codes="169,116"
            atx:keyLabel="©"
            atx:keyLabel2="t"
            atx:popupCharacters="@string/kpt_alternates_for_copyright"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="94,121"
            atx:keyLabel="^"
            atx:keyLabel2="y"
            atx:popupCharacters="@string/kpt_alternates_for_circumflex"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="60,117"
            atx:keyLabel="&lt;"
            atx:keyLabel2="u" />
        <Key
            atx:codes="62,105"
            atx:keyLabel=">"
            atx:keyLabel2="i" />
        <Key
            atx:codes="186,111"
            atx:keyLabel="º"
            atx:keyLabel2="o" />
        <Key
            atx:codes="8226,112"
            atx:keyEdgeFlags="right"
            atx:keyLabel="•"
            atx:keyLabel2="p" />
    </Row>
    <Row>
        <Key
            atx:codes="170,97"
            atx:horizontalGap="5%p"
            atx:keyEdgeFlags="left"
            atx:keyLabel="ª"
            atx:keyLabel2="a" />
        <Key
            atx:codes="91,115"
            atx:keyLabel="["
            atx:keyLabel2="s" />
        <Key
            atx:codes="93,100"
            atx:keyLabel="]"
            atx:keyLabel2="d" />
        <Key
            atx:codes="167,102"
            atx:keyLabel="§"
            atx:keyLabel2="f" />
        <Key
            atx:codes="124,103"
            atx:keyLabel="|"
            atx:keyLabel2="g" />
        <Key
            atx:codes="191,104"
            atx:keyLabel="¿"
            atx:keyLabel2="h" />
        <Key
            atx:codes="171,106"
            atx:keyLabel="«"
            atx:keyLabel2="j" />
        <Key
            atx:codes="187,107"
            atx:keyLabel="»"
            atx:keyLabel2="k" />
        <Key
            atx:codes="61,108"
            atx:keyEdgeFlags="right"
            atx:keyLabel="="
            atx:keyLabel2="l"
            atx:popupCharacters="@string/kpt_alternates_for_equalsign"
            atx:popupKeyboard="@xml/kbd_popup_template" />
    </Row>
    <Row>
        <Key
            atx:codes="-6"
            atx:iconText="@string/kpt_icontext_alt_2"
            atx:keyLabelYPos="0.50"
            atx:keyEdgeFlags="left"
            atx:keyWidth="15%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="42,122"
            atx:keyLabel="*"
            atx:keyLabel2="z"
            atx:popupCharacters="@string/kpt_alternates_for_asterisk"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="37,120"
            atx:keyLabel="%"
            atx:keyLabel2="x"
            atx:popupCharacters="@string/kpt_alternates_for_percentage"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="126,99"
            atx:keyLabel="~"
            atx:keyLabel2="c" />
        <Key
            atx:codes="161,118"
            atx:keyLabel="¡"
            atx:keyLabel2="v" />
        <Key
            atx:codes="176,98"
            atx:keyLabel="°"
            atx:keyLabel2="b" />
        <Key
            atx:codes="92,110"
            atx:keyLabel="\\"
            atx:keyLabel2="n" />
        <Key
            atx:codes="43,109"
            atx:keyLabel="+"
            atx:keyLabel2="m"
            atx:popupCharacters="@string/kpt_alternates_for_plus"
            atx:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:codes="-5"
            atx:keyType="@integer/kpt_functional_key_dont_send_to_core"
            atx:iconText="@string/kpt_icontext_delete"
            atx:keyLabelYPos="0.50"
            atx:isRepeatable="true"
            atx:keyEdgeFlags="right"
            atx:keyWidth="15%p"
            atx:useExternalFont="true" />
    </Row>
    <Row
         atx:keyboardMode="@+id/mode_normal"  
        atx:rowEdgeFlags="bottom"
        atx:keyType="@integer/kpt_functional_key_dont_send_to_core">
        <Key
            atx:codes="-2,2"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:keyWidth="15%p" />
        <Key
            atx:codes="44,-102"
            atx:iconText="@string/kpt_icontext_comma_mic"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32"
            atx:keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:keyWidth="45%p" />
        <Key
            atx:codes="46"
            atx:iconText="@string/kpt_icontext_dot_share"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="1"
            atx:iconText="@string/kpt_icontext_xi_key"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
    </Row>
    <Row 
         atx:keyboardMode="@+id/mode_url" 
        atx:rowEdgeFlags="bottom"
        atx:keyType="@integer/kpt_functional_key_dont_send_to_core" >
        <Key
            atx:codes="-2,2"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:keyWidth="15%p" />
        <Key
            atx:codes="44,-102"
            atx:iconText="@string/kpt_icontext_comma_mic"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32"
            atx:keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:keyWidth="45%p" />
        <Key
            atx:codes="46"
            atx:iconText="@string/kpt_icontext_dot_share"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
       <Key
            atx:codes="1"
            atx:iconText="@string/kpt_icontext_xi_key"
            atx:keyLabelYPos="0.50"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
    </Row>
    
    <Row
         atx:keyboardMode="@+id/mode_normal_hide_xi"  
        atx:rowEdgeFlags="bottom"
        atx:keyType="@integer/kpt_functional_key_dont_send_to_core">
        <Key
            atx:codes="-2,2"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:keyWidth="15%p" />
        <Key
            atx:codes="44,-102"
            atx:iconText="@string/kpt_icontext_comma_mic"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32"
            atx:keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:keyWidth="55%p" />
        <Key
            atx:codes="46"
            atx:iconText="@string/kpt_icontext_dot_share"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
    </Row>
    <Row 
         atx:keyboardMode="@+id/mode_url_hide_xi" 
        atx:rowEdgeFlags="bottom"
        atx:keyType="@integer/kpt_functional_key_dont_send_to_core" >
        <Key
            atx:codes="-2,2"
            atx:keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:keyWidth="15%p" />
        <Key
            atx:codes="44,-102"
            atx:iconText="@string/kpt_icontext_comma_mic"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="32"
            atx:keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:keyWidth="55%p" />
        <Key
            atx:codes="46"
            atx:iconText="@string/kpt_icontext_dot_share"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
        <Key
            atx:codes="10"
            atx:iconText="@string/kpt_icontext_enter_kbhide"
            atx:keyLabel2YPos="0.35"
            atx:keyWidth="10%p"
            atx:useExternalFont="true" />
    </Row>

</Keyboard>