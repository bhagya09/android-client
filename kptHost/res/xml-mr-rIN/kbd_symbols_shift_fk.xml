        <Key
            atx:kpt_codes="59,2330"
            atx:kpt_keyLabel=";"
            atx:kpt_keyLabel2="च" />
        <Key
            atx:kpt_codes="58,2332"
            atx:kpt_keyLabel=":"
            atx:kpt_keyLabel2="ज" />
        <Key
