            atx:kpt_codes="42,-1073"
            atx:kpt_keyLabel="*"
            atx:kpt_keyLabel2="क्ष"
            atx:kpt_popupKeyboard="@xml/popup_ligature2" />
        <Key
            atx:kpt_codes="2416,-1073"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabel="॰"
            atx:kpt_keyLabel2="ज्ञ"
            atx:kpt_popupKeyboard="@xml/popup_ligature3" />
    </Row>
    <Row
        atx:kpt_horizontalGap="0dp"
        atx:kpt_keyWidth="9%p" >
        <Key
            atx:kpt_codes="-6"
			atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="..."
            atx:kpt_popupKeyboard="@xml/popup_symbols" />
