        <Key
            atx:kpt_codes="63,2337"
            atx:kpt_keyLabel="\?"
            atx:kpt_keyLabel2="ड" />
        <Key
            atx:kpt_codes="40,2340"
            atx:kpt_keyLabel="("
            atx:kpt_keyLabel2="त"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_leftparenthesis"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="41,2342"
            atx:kpt_keyLabel=")"
            atx:kpt_keyLabel2="द"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_rightparenthesis"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="43,2344"
            atx:kpt_keyLabel="+"
            atx:kpt_keyLabel2="न"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_plus"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
