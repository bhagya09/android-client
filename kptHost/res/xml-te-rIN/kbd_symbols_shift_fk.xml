<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:atx="http://schemas.android.com/apk/res-auto"
    atx:kpt_horizontalGap="0dp"
    
    atx:kpt_fontSizeApplicable="false"
    atx:kpt_fontStyleApplicable="false"
    atx:kpt_isGlideSupported = "false"
    atx:kpt_keyType="@integer/kpt_key_type_normal"
    atx:kpt_isRTL="false"
    
    atx:kpt_keyLabel2TextSize="@dimen/kpt_keyLabel2TextSize"
    atx:kpt_keyLabel2XPos="@dimen/kpt_keyLabel2XPos"
    atx:kpt_keyLabel2YPos="@dimen/kpt_keyLabel2YPos"
    
    atx:kpt_keyLabelTextSize="@dimen/kpt_keyLabelTextSize"
    atx:kpt_keyLabelXPos="@dimen/kpt_keyLabelXPos"
    atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelYPos"
    
    atx:kpt_keyHeight="@dimen/kpt_key_height_dk_fk"
    atx:kpt_keyWidth="10%p"
    atx:kpt_verticalGap="@dimen/kpt_vertical_gap" >

    <Row
        atx:kpt_horizontalGap="0dp"
        atx:kpt_keyWidth="9%p" >
        <Key
            atx:kpt_codes="49,3149"
            atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="1"
            atx:kpt_keyLabel2="్"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_one"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="50,3134"
            atx:kpt_keyLabel="2"
            atx:kpt_keyLabel2="ా"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_two"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="51,3135"
            atx:kpt_keyLabel="3"
            atx:kpt_keyLabel2="ి"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_three"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="52,3136"
            atx:kpt_keyLabel="4"
            atx:kpt_keyLabel2="ీ"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_four"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="53,3137"
            atx:kpt_keyLabel="5"
            atx:kpt_keyLabel2="ు"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_five"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="54,3138"
            atx:kpt_keyLabel="6"
            atx:kpt_keyLabel2="ూ"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_six"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="55,3142"
            atx:kpt_keyLabel="7"
            atx:kpt_keyLabel2="ె"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_seven"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="56,3143"
            atx:kpt_keyLabel="8"
            atx:kpt_keyLabel2="ే"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_eight"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="57,3144"
            atx:kpt_keyLabel="9"
            atx:kpt_keyLabel2="ై"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_nine"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="48,3146"
            atx:kpt_keyLabel="0"
            atx:kpt_keyLabel2="ొ"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_zero"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="61,3147"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabel="="
            atx:kpt_keyLabel2="ో"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_equalsign"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
    </Row>
    <Row
        atx:kpt_horizontalGap="0dp"
        atx:kpt_keyWidth="9%p" >
        <Key
            atx:kpt_codes="33,3148"
            atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabel="!"
            atx:kpt_keyLabel2="ౌ" />
        <Key
            atx:kpt_codes="59,3093"
            atx:kpt_keyLabel=";"
            atx:kpt_keyLabel2="క" />
        <Key
            atx:kpt_codes="58,3095"
            atx:kpt_keyLabel=":"
            atx:kpt_keyLabel2="గ" />
        <Key
            atx:kpt_codes="38,3097"
            atx:kpt_keyLabel="&amp;"
            atx:kpt_keyLabel2="ఙ" />
        <Key
            atx:kpt_codes="94,3098"
            atx:kpt_keyLabel="^"
            atx:kpt_keyLabel2="చ" />
        <Key
            atx:kpt_codes="124,3100"
            atx:kpt_keyLabel="|"
            atx:kpt_keyLabel2="జ" />
        <Key
            atx:kpt_codes="45,3103"
            atx:kpt_keyLabel="-"
            atx:kpt_keyLabel2="ట"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_hyphen"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="39,3105"
            atx:kpt_keyLabel="\&apos;"
            atx:kpt_keyLabel2="డ" />
        <Key
            atx:kpt_codes="34,3108"
            atx:kpt_keyLabel="&quot;"
            atx:kpt_keyLabel2="త"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_quote"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="43,3110"
            atx:kpt_keyLabel="+"
            atx:kpt_keyLabel2="ద"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_plus"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="35,3074"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_keyLabel="#"
            atx:kpt_keyLabel2="ం" />
    </Row>
    <Row
        atx:kpt_horizontalGap="0dp"
        atx:kpt_keyWidth="9%p" >
        <Key
            atx:kpt_codes="-6"  atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyEdgeFlags="left"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_keyLabel="..."
            atx:kpt_popupKeyboard="@xml/popup_symbols" />
        <Key
            atx:kpt_codes="-7,3114"
            atx:kpt_keyLabel=":-)"
            atx:kpt_keyLabel2="ప"
            atx:kpt_keyOutputText=":-) "
            atx:kpt_popupKeyboard="@xml/popup_smileys_qwerty" />
        <Key
            atx:kpt_codes="64,3116"
            atx:kpt_keyLabel="\@"
            atx:kpt_keyLabel2="బ" />
        <Key
            atx:kpt_codes="40,3118"
            atx:kpt_keyLabel="("
            atx:kpt_keyLabel2="మ"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_leftparenthesis"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="41,3128"
            atx:kpt_keyLabel=")"
            atx:kpt_keyLabel2="స"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_rightparenthesis"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="47,3120"
            atx:kpt_keyLabel="/"
            atx:kpt_keyLabel2="ర"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_slash"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="8377,3122"
            atx:kpt_keyLabel="₹"
            atx:kpt_keyLabel2="ల"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_currency"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template" />
        <Key
            atx:kpt_codes="42,3125"
            atx:kpt_keyLabel="*"
            atx:kpt_keyLabel2="వ" />
        <Key
            atx:kpt_codes="63,3127"
            atx:kpt_keyLabel="\?"
            atx:kpt_keyLabel2="ష" />
        <Key
            atx:kpt_codes="126,3119"
            atx:kpt_keyLabel="~"
            atx:kpt_keyLabel2="య" />
        <Key
            atx:kpt_codes="-5"
            atx:kpt_keyType="@integer/kpt_functional_key_dont_send_to_core"
            atx:kpt_iconText="@string/kpt_icontext_delete"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_isRepeatable="true"
            atx:kpt_keyEdgeFlags="right"
            atx:kpt_useExternalFont="true" />
    </Row>
    
    
    
    <Row
         atx:kpt_keyboardMode="@+id/mode_normal_hide_xi"  
        atx:kpt_rowEdgeFlags="bottom"
        atx:kpt_keyType="@integer/kpt_functional_key_dont_send_to_core">
        <Key
            atx:kpt_codes="-2,-136"
            atx:kpt_keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelYPos_atx"
            atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyWidth="15%p" />
        <Key
            atx:kpt_codes="44,-102"
            atx:kpt_iconText="@string/kpt_icontext_comma_mic"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="10%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="32"
            atx:kpt_keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:kpt_keyWidth="54.5%p" />
        <Key
            atx:kpt_codes="46"
            atx:kpt_iconText="@string/kpt_icontext_dot_share"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_dot"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="10%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="10"
            atx:kpt_iconText="@string/kpt_icontext_enter_kbhide"
            atx:kpt_keyWidth="10%p"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>
    <Row 
         atx:kpt_keyboardMode="@+id/mode_url_hide_xi" 
        atx:kpt_rowEdgeFlags="bottom"
        atx:kpt_keyType="@integer/kpt_functional_key_dont_send_to_core" >
        <Key
            atx:kpt_codes="-2,-136"
            atx:kpt_keyIcon="@drawable/kpt_adaptxt_settings_fk"
            atx:kpt_keyLabelYPos="@dimen/kpt_keyLabelYPos_atx"
            atx:kpt_horizontalGap="0.5%p"
            atx:kpt_keyWidth="15%p" />
        <Key
            atx:kpt_codes="44,-102"
            atx:kpt_iconText="@string/kpt_icontext_comma_mic"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="10%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="32"
            atx:kpt_keyIcon="@drawable/kpt_sym_keyboard_space_dk_fk"
            atx:kpt_keyWidth="54.5%p" />
        <Key
            atx:kpt_codes="46"
            atx:kpt_iconText="@string/kpt_icontext_dot_share"
            atx:kpt_popupCharacters="@string/kpt_alternates_for_dot"
            atx:kpt_popupKeyboard="@xml/kbd_popup_template"
            atx:kpt_keyLabel2YPos="0.35"
            atx:kpt_keyWidth="10%p"
            atx:kpt_useExternalFont="true" />
        <Key
            atx:kpt_codes="10"
            atx:kpt_iconText="@string/kpt_icontext_enter_kbhide"
            atx:kpt_keyWidth="10%p"
            atx:kpt_keyLabelYPos="0.50"
            atx:kpt_useExternalFont="true" />
    </Row>

</Keyboard>